                        </ComboBox>

                        <ComboBox x:Name="cm_PROG01CHN" Width="35" Height="AUTO" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Center"  Grid.Column="4" Grid.Row="1">
                            <ComboBoxItem>1</ComboBoxItem>
                            <ComboBoxItem>2</ComboBoxItem>
