<Window x:Class="JF17StandaloneDTC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:JF17StandaloneDTC"
        mc:Ignorable="d"
        Title="JF-17 Standalone DTC v1 by Bailey" Height="305" Width="880">
    <Window.Resources>
        <Style TargetType="Label">
            <Setter Property="Foreground" Value="White" />
        </Style>
        <Style TargetType="ScrollBar">
            <Setter Property="Background" Value="Black"/>
        </Style>
        <!--<Style TargetType="Button">
            <Setter Property="Background" Value="Black"/>
        </Style>-->
        <Style TargetType="{x:Type Button}">
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="14"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border Background="{TemplateBinding Background}">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                        </Border>
                    </ControlTemplate>

                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="White"/>
                    <Setter Property="Foreground" Value="Black"/>
                </Trigger>
                <Trigger Property="IsPressed" Value="True">
                    <Setter Property="Background" Value="Gray"/>
                    <Setter Property="Foreground" Value="Black"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>

    <Grid Background="Black">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5" />
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="1.4*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="25" />
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        

        <Image x:Name="MainMenuLogo"  Source="/Assets/MainMenulogoDTC.png" Grid.Column="1" Grid.Row="0"  Height="70" Margin="0,5" MouseLeftButtonDown="MainMenuLogo_MouseLeftButtonDown"/>
        
        <!--<Button Content="Export" Grid.Column="1" Grid.Row="1" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Visibility="Hidden"/>-->

        <Button x:Name="button_selectFile" Content="Select options.lua" Grid.Column="1" Grid.Row="1" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="125" Click="button_selectFile_Click" >
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="1"/>
                    <Setter Property="BorderBrush" Value="#ffc600"/>
                    <Setter Property="BorderThickness" Value="0,0,1,1"/>
                </Style>
            </Button.Resources>
        </Button>

        <Button x:Name="button_selectFile2" Content="Select JF-17 Doc" Grid.Column="1" Grid.Row="2" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="125" Click="button_selectFile2_Click" >
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="1"/>
                    <Setter Property="BorderBrush" Value="#ffc600"/>
                    <Setter Property="BorderThickness" Value="0,0,1,1"/>
                </Style>
            </Button.Resources>
        </Button>

        <!--<Button Content="Spacer Button1" Grid.Column="1" Grid.Row="2" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Visibility="Hidden"/>-->

        <Button x:Name="button_import" Content="Import" Grid.Column="1" Grid.Row="3" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Click="button_import_Click" >
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="1"/>
                    <Setter Property="BorderBrush" Value="#ffc600"/>
                    <Setter Property="BorderThickness" Value="0,0,1,1"/>
                </Style>
            </Button.Resources>
        </Button>

        <!--<TextBox Text="Drag and drop Options.Lua Here" Grid.Column="1" Grid.Row="4" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" AllowDrop="True" PreviewDragOver="TextBox_PreviewDragOver" Drop="TextBox_Drop" />-->

        <Button x:Name="button_export" Content="Export" Grid.Column="1" Grid.Row="5" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Click="button_export_Click" >
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="1"/>
                    <Setter Property="BorderBrush" Value="#ffc600"/>
                    <Setter Property="BorderThickness" Value="0,0,1,1"/>
                </Style>
            </Button.Resources>
        </Button>

        <Button x:Name="button_loadBackup" Content="Import Backup" Grid.Column="1" Grid.Row="4" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Click="button_loadBackup_Click" >
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="1"/>
                    <Setter Property="BorderBrush" Value="#ffc600"/>
                    <Setter Property="BorderThickness" Value="0,0,1,1"/>
                </Style>
            </Button.Resources>
        </Button>

        <Button x:Name="button_saveBackup" Content="Export Backup" Grid.Column="1" Grid.Row="6" Margin="5" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Click="button_saveBackup_Click" >
            <Button.Resources>
                <Style TargetType="Border">
                    <Setter Property="CornerRadius" Value="1"/>
                    <Setter Property="BorderBrush" Value="#ffc600"/>
                    <Setter Property="BorderThickness" Value="0,0,1,1"/>
                </Style>
            </Button.Resources>
        </Button>


        <TabControl Background="Black" BorderBrush="Black"
                    Grid.ColumnSpan="10" Grid.RowSpan="10" Grid.Column="2" Grid.Row="0" Margin="5">

            <TabControl.Resources>
                <Style TargetType="TabItem">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="TabItem">
                                <Border Name="Border" BorderThickness="1,1,1,1" BorderBrush="Black" CornerRadius="1,1,1,1" Margin="2,0" Background="Black">
                                    <ContentPresenter x:Name="ContentSite"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        ContentSource="Header"
                                        Margin="10,2"/>
                                </Border>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsSelected" Value="True">
                                        <Setter TargetName="Border" Property="Background" Value="black" />
                                    </Trigger>
                                    <Trigger Property="IsSelected" Value="True">
                                        <Setter TargetName="Border" Property="BorderBrush" Value="#ffc600" />
                                    </Trigger>

                                    <Trigger Property="IsSelected" Value="False">
                                        <Setter TargetName="Border" Property="Background" Value="black" />
                                    </Trigger>
                                    <Trigger Property="IsSelected" Value="False">
                                        <Setter TargetName="Border" Property="BorderBrush" Value="gray" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </TabControl.Resources>

            <TabItem Header="General" Foreground="White" BorderBrush="Black" Background="Black">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>


                    <Label Content="Cockpit Model" VerticalAlignment="Center" Grid.Column="0" Grid.Row="0"/>
                    <ComboBox x:Name="general_CPTModel" Width="100" Height="AUTO" Margin="-10" VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Column="1" Grid.Row="0">
                        <ComboBoxItem IsSelected="True">Visual Quality</ComboBoxItem>
                        <ComboBoxItem>Performance</ComboBoxItem>
                    </ComboBox>


                    <Label Content="KYBD Pitch Control" VerticalAlignment="Center" Grid.Column="2" Grid.Row="0"/>
                    <ComboBox x:Name="general_KYBDPITCH" Width="130" Height="AUTO" Margin="-10" VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Column="3" Grid.Row="0">
                        <ComboBoxItem IsSelected="True">AUTO RE-CENTER</ComboBoxItem>
                        <ComboBoxItem>ACCUMULATIVE</ComboBoxItem>
                        <ComboBoxItem>BLENDED</ComboBoxItem>
                    </ComboBox>

                    <Label Content="Gun Sight" VerticalAlignment="Center" Grid.Column="0" Grid.Row="1"/>
                    <ComboBox x:Name="general_GUNSIGHT" Width="55" Height="AUTO" Margin="-10" VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Column="1" Grid.Row="1">
                        <ComboBoxItem IsSelected="True">LCOS</ComboBoxItem>
                        <ComboBoxItem>SSLC</ComboBoxItem>
                        <ComboBoxItem>SS</ComboBoxItem>
                    </ComboBox>

                    <Label Content="Gun Limiter" VerticalAlignment="Center"  Grid.Column="2" Grid.Row="1"/>
                    <ComboBox x:Name="general_GUNLIMIT" Width="55" Height="AUTO" Margin="-10" VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Column="3" Grid.Row="1">
                        <ComboBoxItem>OFF</ComboBoxItem>
                        <ComboBoxItem>0.2</ComboBoxItem>
                        <ComboBoxItem IsSelected="True">0.5</ComboBoxItem>
                    </ComboBox>

                    <Label Content="Voice Locale" VerticalAlignment="Center" Grid.Column="0" Grid.Row="2"/>
                    <ComboBox x:Name="general_VOICELOCALE" Width="100" Height="AUTO" Margin="-10" VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Column="1" Grid.Row="2">
                        <ComboBoxItem IsSelected="True">EN 1</ComboBoxItem>
                        <ComboBoxItem>EN 2</ComboBoxItem>
                        <ComboBoxItem>CN 1</ComboBoxItem>
                        <ComboBoxItem>CN 2</ComboBoxItem>
                        <ComboBoxItem>CUSTOMIZED</ComboBoxItem>
                    </ComboBox>

                    <Label Content="DMAP Type Locale" VerticalAlignment="Center" Grid.Column="2" Grid.Row="2"/>
                    <ComboBox x:Name="general_DMAPTYPE" Width="130" Height="AUTO" Margin="-10" VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Column="3" Grid.Row="2">
                        <ComboBoxItem IsSelected="True">RASTER CHARTS</ComboBoxItem>
                        <ComboBoxItem>MAP ALT</ComboBoxItem>
                    </ComboBox>

                    <Label Content="Music Number" VerticalAlignment="Center" Grid.Column="0" Grid.Row="3"/>
                    <xctk:DecimalUpDown x:Name="general_MUSICNUM_SLIDER" Width="55" Height="19" DefaultValue="10" Maximum="30" Minimum="0" Margin="-10"
                                            ParsingNumberStyle="Integer" DisplayDefaultValueOnEmptyText="True" MaxLength="2"
                                            VerticalAlignment="Center" HorizontalAlignment="Left"  Grid.Column="1" Grid.Row="3"/>

                    
                    <Button x:Name="button_musicFolder" Content="Open Music Folder" Grid.Column="1" Grid.Row="3" VerticalAlignment="Center" 
                            HorizontalAlignment="Right" Width="120" Margin="5,0" Click="button_musicFolder_Click">
                        <Button.Resources>
                            <Style TargetType="Border">
                                <Setter Property="CornerRadius" Value="1"/>
                                <Setter Property="BorderBrush" Value="#ffc600"/>
                                <Setter Property="BorderThickness" Value="0,0,1,1"/>
                            </Style>
                        </Button.Resources>
                    </Button>

                    <CheckBox x:Name="general_HiddenStick" Content="Hide Stick" IsChecked="False" Foreground="White" Grid.Column="0" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                    <CheckBox x:Name="general_AntZoomInv" Content="T6 Opposite Axis Dir" IsChecked="False" Foreground="White" Grid.Column="1" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                    <CheckBox x:Name="general_RemoveProbe" Content="Remove AAR Probe" IsChecked="False" Foreground="White" Grid.Column="2" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                    <CheckBox x:Name="general_TestingChg" Content="Testing Feature" IsChecked="True" Foreground="White" Grid.Column="3" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                    <CheckBox x:Name="general_IcingOnCake" Content="Icing On the Cake" IsChecked="True" Foreground="White" Grid.Column="0" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Left"/>

                </Grid>
            </TabItem>

            <TabItem Header="Controls" Foreground="White" BorderBrush="Black" Background="Black">

                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <Label Content="TDC Deadzone" VerticalAlignment="Center" Grid.Row="0" Grid.Column="0"/>
                    <xctk:DecimalUpDown x:Name="controls_TDC_DEADZONE" Width="65" Height="19" DefaultValue="0.01" Maximum="0.1" Minimum="0" Margin="-10" 
                                            ParsingNumberStyle="AllowDecimalPoint" DisplayDefaultValueOnEmptyText="True" MaxLength="4"
                                            VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="0"  Grid.Column="1" Increment="0.01"/>

                    <Label Content="HUD TDC Slew Rate" VerticalAlignment="Center" Grid.Row="0" Grid.Column="2"/>
                    <xctk:DecimalUpDown x:Name="controls_HUD_TDC" Width="65" Height="19" DefaultValue="1" Maximum="2" Minimum="0.2" Margin="-10" Increment="0.1" 
                                            ParsingNumberStyle="AllowDecimalPoint" DisplayDefaultValueOnEmptyText="True" MaxLength="4"
                                            VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="0"  Grid.Column="3" />

                    <Label Content="DL POD TDC Slew Rate" VerticalAlignment="Center" Grid.Row="1" Grid.Column="0"/>
                    <xctk:DecimalUpDown x:Name="controls_DLPOD_TDC" Width="65" Height="19" DefaultValue="1" Maximum="2" Minimum="0.2" Margin="-10" Increment="0.1" 
                                            ParsingNumberStyle="AllowDecimalPoint" DisplayDefaultValueOnEmptyText="True" MaxLength="4"
                                            VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="1"  Grid.Column="1" />

                    <Label Content="WMD7 TDC Slew Rate" VerticalAlignment="Center" Grid.Row="1" Grid.Column="2"/>
                    <xctk:DecimalUpDown x:Name="controls_WMD_TDC" Width="65" Height="19" DefaultValue="1" Maximum="2" Minimum="0.2" Margin="-10" Increment="0.1" 
                                            ParsingNumberStyle="AllowDecimalPoint" DisplayDefaultValueOnEmptyText="True" MaxLength="4"
                                            VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="1"  Grid.Column="3" />

                    <Label Content="C701 TDC Slew Rate" VerticalAlignment="Center" Grid.Row="2" Grid.Column="0"/>
                    <xctk:DecimalUpDown x:Name="controls_TVIR_TDC" Width="65" Height="19" DefaultValue="1" Maximum="2" Minimum="0.2" Margin="-10" Increment="0.1" 
                                            ParsingNumberStyle="AllowDecimalPoint" DisplayDefaultValueOnEmptyText="True" MaxLength="4"
                                            VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="2"  Grid.Column="1" />

                    <Label Content="Radar TDC Slew Rate" VerticalAlignment="Center" Grid.Row="2" Grid.Column="2"/>
                    <xctk:DecimalUpDown x:Name="controls_RDR_TDC" Width="65" Height="19" DefaultValue="1" Maximum="2" Minimum="0.2" Margin="-10" Increment="0.1" 
                                            ParsingNumberStyle="AllowDecimalPoint" DisplayDefaultValueOnEmptyText="True" MaxLength="4"
                                            VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="2"  Grid.Column="3" />

                    <Label Content="Radar Elev Slew Rate" VerticalAlignment="Center" Grid.Row="3" Grid.Column="2"/>
                    <xctk:DecimalUpDown x:Name="controls_RDR_ELEV" Width="65" Height="19" DefaultValue="1" Maximum="2" Minimum="0.2" Margin="-10" Increment="0.1" 
                                            ParsingNumberStyle="AllowDecimalPoint" DisplayDefaultValueOnEmptyText="True" MaxLength="4"
                                            VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Row="3"  Grid.Column="3" />
                </Grid>
            </TabItem>

            <TabItem Header="Countermeasures" Foreground="White" Background="Black" BorderBrush="Black">

                <StackPanel>

                    <Grid Background="Black">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <!--<ColumnDefinition Width="17"/>-->
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <!--<RowDefinition Height="Auto"/>-->
                        </Grid.RowDefinitions>

                        <Label Content="OESP" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="0" />
                        <Label Content="Type" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="1" />
                        <Label Content="Chaff SQ" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="2" />
                        <Label Content="Chaff SI" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="3" />
                        <Label Content="Chaff BQ" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="4" />
                        <Label Content="Chaff BI" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="5" />
                        <Label Content="Flare SQ" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="6" />
                        <Label Content="Flare SI" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="7" />
                        <Label Content="Flare BQ" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="8" />
                        <Label Content="Flare BI" HorizontalAlignment="Center" VerticalAlignment="Top" Grid.Column="9" />

                    </Grid>


                    <!--<Grid Height="200">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
