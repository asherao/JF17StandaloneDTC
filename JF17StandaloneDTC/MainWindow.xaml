                        </ComboBox>



                        <Label Content="PROG02" Grid.Row="2" Grid.Column="0" HorizontalAlignment="Center" />

                        <ComboBox x:Name="cm_PROG02TYPE" Width="65" Height="AUTO" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Center"  Grid.Column="1" Grid.Row="2">
                            <ComboBoxItem IsSelected="True">CH</ComboBoxItem>
                            <ComboBoxItem>FL</ComboBoxItem>
                            <ComboBoxItem>CH+FL</ComboBoxItem>
                        </ComboBox>

                        <ComboBox x:Name="cm_PROG02CHR" Width="35" Height="AUTO" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Center"  Grid.Column="2" Grid.Row="2">
                            <ComboBoxItem>1</ComboBoxItem>
                            <ComboBoxItem>2</ComboBoxItem>
                            <ComboBoxItem IsSelected="True">3</ComboBoxItem>
