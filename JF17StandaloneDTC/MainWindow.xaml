                        <ComboBox x:Name="cm_PROG01FLR" Width="35" Height="AUTO" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Center"  Grid.Column="6" Grid.Row="1">
                            <ComboBoxItem IsSelected="True">1</ComboBoxItem>
