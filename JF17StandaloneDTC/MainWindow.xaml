                            <ComboBoxItem>4</ComboBoxItem>
                            <ComboBoxItem>5</ComboBoxItem>
                            <ComboBoxItem>6</ComboBoxItem>
                        </ComboBox>

                        <ComboBox x:Name="cm_PROG02CHBI" Width="50" Height="AUTO" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Center"  Grid.Column="5" Grid.Row="2">
                            <ComboBoxItem IsSelected="True">0.0</ComboBoxItem>
                            <ComboBoxItem>0.2</ComboBoxItem>
                            <ComboBoxItem>0.5</ComboBoxItem>
